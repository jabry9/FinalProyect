{
  "name": "Anuncio",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "titulo": {
      "type": "string",
      "required": true
    },
    "presupMAX": {
      "type": "number",
      "required": true
    },
    "materialsINC": {
      "type": "boolean",
      "required": true
    },
    "location": {
      "type": "geopoint",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "visitas": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "usuario": {
      "type": "belongsTo",
      "model": "Usuario",
      "foreignKey": "",
      "imageable": {
        "type": "belongsTo",
        "polymorphic": true
      }
    },
    "empresa": {
      "type": "belongsTo",
      "model": "Empresa",
      "foreignKey": "",
      "imageable": {
        "type": "belongsTo",
        "polymorphic": true
      }
    },
    "empresas": {
      "type": "hasMany",
      "model": "Empresa",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      },
      "through": "Solicitud"
    },
    "categories": {
      "type": "hasOne",
      "model": "Category",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "solicitudAcept": {
      "type": "belongsTo",
      "model": "Solicitud",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
